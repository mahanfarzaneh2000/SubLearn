{
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run start",
    "devPath": "http://localhost:3000",
    "distDir": "../build"
  },
  "package": {
    "productName": "sublearn",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false
    },
    "bundle": {
      "active": true,
      "category": "Education",
      "publisher": "Mahan Farzaneh",
      "copyright": "Copyright 2023-2024 Mahan Farzaneh",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "ir.mahanfr.sublearn",
      "longDescription": "This application designed to teach you english as a seconed language by analysing english words and extracting the ones that are chalenging for your level",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [
        "./db.sqlite3"
      ],
      "shortDescription": "learn english using subtitles and movies",
      "targets": "all",
      "windows": {
        "webviewInstallMode": {
            "type": "embedBootstrapper"
        },
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 720,
        "resizable": true,
        "title": "Sublearn",
        "width": 800
      }
    ]
  }
}
